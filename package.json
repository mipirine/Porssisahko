{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^1.6.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-charts": "^4.0.1",
    "react-scripts": "^3.0.1"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "build-prod": "npm install && npm update && npm run build && npm install && npm update && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "api": "nodemon --verbose --watch \"api\" --ext \"js,json\" --exec api",
    "client": "npm start",
    "dev": "concurrently \"npm run src\" \"npm run api\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^0.2.1"
  }
}
